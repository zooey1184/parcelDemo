template>\r\n  <div class=\"o-card_wrap\">\r\n    <transition name='bounce'>\r\n      <img class=\"checked\" v-if='checked' src=\"./img/cked.png\" alt=\"\">\r\n    </transition>\r\n    <img class=\"img_middle\" v-if='(status==\"1\" || status===1)' src=\"./img/over.png\" alt=\"\">\r\n    <img class=\"img_middle\" v-if='(status==\"2\" || status==\"3\" || status==\"4\" || status===2)' src=\"./img/used.png\" alt=\"\">\r\n    <div class=\"title\">\r\n      <div class=\"w_6\">\r\n        <p :style='{color: (status==\"0\" || status===0)? \"#333\" : \"#999\"}'>{{title}}</p>\r\n      </div>\r\n      <div class=\"w_4\">\r\n        <span class=\"sym\" :style='{color: (status==\"0\" || status===0)? \"#FF7800\" : \"#999\"}'>￥</span>\r\n        <span class=\"num\" :style='{color: (status==\"0\" || status===0)? \"#FF7800\" : \"#999\"}'>{{amount}}</span>\r\n      </div>\r\n    </div>\r\n    <div class=\"desc\" :style='{color: (status==\"0\" || status===0)? \"#666\" : \"#999\"}'>\r\n      <p class=\"w_65\">有效期至{{expire_time}}</p>\r\n      <p class=\"w_35\">{{remark}}</p>\r\n    </div>\r\n\r\n    <div class=\"time\" :style='{color: (status==\"0\" || status===0)? \"#666\" : \"#999\"}'>\r\n      <p>{{desc}}</p>\r\n      <div class=\"circle c_left\"></div>\r\n      <div class=\"circle c_right\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'card-item',\r\n  props: {\r\n    checked: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    status: {\r\n      type: [String, Number],\r\n      default: '0' // 0未使用、1已过期、2已使用、3已抵扣、4已失效\r\n    },\r\n    state: {\r\n      type: String,\r\n      default: 'y', // y=>yes  o=>over  n=>no\r\n    },\r\n    title: {\r\n      type: String,\r\n      default: 'y', \r\n    },\r\n    amount: {\r\n      type: [String, Number],\r\n      default: 0, \r\n    },\r\n    expire_time: {\r\n      type: String,\r\n      default: '', \r\n    },\r\n    remark: {\r\n      type: String,\r\n      default: '', \r\n    },\r\n    desc: {\r\n      type: String,\r\n      default: '请在签约时选择使用红包，逾期还款红包将失效'\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.select_none {\r\n  -moz-user-select:none;\r\n  -webkit-user-select:none;\r\n  -ms-user-select:none;\r\n  -khtml-user-select:none;\r\n  user-select:none\r\n}\r\n.o-card_wrap {\r\n  position: relative;\r\n  .select_none;\r\n  .checked {\r\n    position: absolute;\r\n    width: 30px;\r\n    left: 0;\r\n    top: 0;\r\n  }\r\n  .img_middle {\r\n    width: 66px;\r\n    height: 66px;\r\n    position: absolute;\r\n    left: 43%;\r\n    top: 14%;\r\n    @media (max-width: 330px) {\r\n      width: 50px;\r\n      height: 50px;\r\n    }\r\n  }\r\n  .w_65 {\r\n    width: 65%;\r\n    text-align: left;\r\n  }\r\n  .w_35 {\r\n    width: 35%;\r\n    text-align: right;\r\n  }\r\n  .w_6 {\r\n    width: 60%;\r\n    text-align: left;\r\n  }\r\n  .w_4 {\r\n    width: 40%;\r\n    text-align: right;\r\n  }\r\n  .title {\r\n    padding: 23px;\r\n    padding-bottom: 5px;\r\n    line-height: 24px;\r\n    &>div {\r\n      display: inline-block;\r\n      vertical-align: bottom;\r\n    }\r\n    .sym {\r\n      font-size: 19px;\r\n      @media (max-width: 330px) {\r\n        font-size: 16px;\r\n      }\r\n    }\r\n    .num {\r\n      font-size: 32px;\r\n      @media (max-width: 330px) {\r\n        font-size: 28px;\r\n      }\r\n    }\r\n  }\r\n  .desc {\r\n    padding: 0 23px;\r\n    line-height: 13px;\r\n    font-size: 13px;\r\n    color: #666;\r\n    margin-bottom: 27px;\r\n    &>p {\r\n      display: inline-block;\r\n      vertical-align: bottom\r\n    }\r\n  }\r\n  .time {\r\n    height: 32px;\r\n    font-size: 12px;\r\n    white-space: nowrap;\r\n    color: #666;\r\n    position: relative;\r\n    padding-top: 9px;\r\n    padding-left: 15px;\r\n    line-height: 13px;\r\n    box-sizing: border-box;\r\n    border-top: 1px dashed #ddd;\r\n    @media (max-width: 330px) {\r\n      p {\r\n        font-size: 12px;\r\n        transform: scale(.9);\r\n        transform-origin: 0 50%;\r\n        white-space: nowrap;\r\n      }\r\n    }\r\n    .circle {\r\n      width: 15px;\r\n      height: 15px;\r\n      border-radius: 20px;\r\n      position: absolute;\r\n      background: rgb(245, 245, 245);\r\n      top: 0;\r\n    }\r\n    .c_left {\r\n      left: 0;\r\n      transform: translate(-50%, -50%);\r\n    }\r\n    .c_right {\r\n      right: 0;\r\n      transform: translate(50%, -50%);\r\n    }\r\n  }\r\n}\r\n</style>\r\n"},"lineCount":null}},"error":null,"hash":"2d4227257079387ad0a0f7e2361db1d2","cacheData":{"env":{}}}