d":{"line":102,"column":0}},{"source":"views/loanMarket/index.vue","name":null,"original":{"line":91,"column":0},"generated":{"line":102,"column":9}},{"source":"views/loanMarket/index.vue","name":null,"original":{"line":91,"column":0},"generated":{"line":102,"column":28}},{"source":"views/loanMarket/index.vue","name":null,"original":{"line":92,"column":0},"generated":{"line":103,"column":0}},{"source":"views/loanMarket/index.vue","name":null,"original":{"line":35,"column":0},"generated":{"line":104,"column":0}}],"sources":{"views/loanMarket/index.vue":"<template>\r\n  <page :state='pageState'>\r\n    <div class=\"loan_market\">\r\n      <div class=\"banner\" v-if='banner'>\r\n        <slide :showDot='false'>\r\n          <div v-for='(item, index) in banner' :key='index' @click='goUrlFn(item)'>\r\n            <img :src=\"item.image\" :alt=\"item.desc\">\r\n          </div>\r\n        </slide>\r\n      </div>\r\n      <div class=\"market_contain\" v-if='market'>\r\n        <div class=\"item\" v-for='(item, index) in market' :key='index'>\r\n          <h3>{{item.market_type}}</h3>\r\n          <div class=\"item_i_wrap\">\r\n            <div class=\"i_item\" v-for='(i, k) in item.business' :key='k' @click='goItemFn(i)'>\r\n              <div class=\"title_pane\">\r\n                <span class=\"title_left\">{{i.name}}</span>\r\n                <span class=\"title_right\">{{i.loan_desc}}</span>\r\n              </div>\r\n              <img :src=\"i.logo\" alt=\"\">\r\n              <p class=\"desc\">{{i.interest_desc}}</p>\r\n              <p class=\"slogan\">{{i.slogan}}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </page>\r\n</template>\r\n\r\n<script>\r\nimport api from '../../api'\r\nimport slide from '../../../../components/Picker/slide.vue'\r\n\r\nexport default {\r\n  data: ()=> ({\r\n    banner: null,\r\n    market: null,\r\n    pageState: 'loading'\r\n  }),\r\n  components: {\r\n    slide\r\n  },\r\n  methods: {\r\n    getBannerFn() {\r\n      const self = this\r\n      this.$ajax({\r\n        url: api().get_market_banners,\r\n        success: res=> {\r\n          if(res.success) {\r\n            self.banner = res.ret\r\n          }else {\r\n            self.$toast.show(res.msg)\r\n          }\r\n        },\r\n        error: (e)=> {\r\n          console.log(e);\r\n          self.pageState = 'error'\r\n        }\r\n      })\r\n    },\r\n    get_business_infoFn() {\r\n      const self = this\r\n      this.$ajax({\r\n        url: api().get_market_infos,\r\n        success: res=> {\r\n          if(res.success) {\r\n            self.market = res.ret\r\n            setTimeout(()=> {\r\n              self.pageState = 'success'\r\n            }, 50)\r\n          }else {\r\n            self.$toast.show(res.msg)\r\n          }\r\n        },\r\n        error: (e)=> {\r\n          console.log(e);\r\n          self.pageState = 'error'\r\n        }\r\n      })\r\n    },\r\n    goUrlFn(item) {\r\n      window.location.href = item.url\r\n    },\r\n    goItemFn(i) {\r\n      window.location.href = i.url\r\n    }\r\n  },\r\n  created() {\r\n    this.getBannerFn()\r\n    this.get_business_infoFn()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.loan_market {\r\n  .banner {\r\n    width: 100%;\r\n    overflow: hidden;\r\n    position: relative;\r\n  }\r\n}\r\n.market_contain {\r\n  .item {\r\n    background: #fff;\r\n    padding: 10px 0;\r\n    margin: 10px 0;\r\n    h3 {\r\n      font-size: 15px;\r\n      text-align: center;\r\n      margin-bottom: 10px;\r\n      font-weight: 500\r\n    }\r\n  }\r\n  .item_i_wrap {\r\n    display: flex;\r\n    align-items: center;\r\n    // justify-content: space-around;\r\n    img {\r\n      width: 45px;\r\n      height: 45px;\r\n      display: block;\r\n      margin: 10px auto;\r\n    }\r\n  }\r\n  .i_item {\r\n    display: inline-block;\r\n    vertical-align: top;\r\n    width: 28%;\r\n    margin-left: 5%;\r\n    border: 1px solid #eee;\r\n    padding: 5px 0;\r\n    border-radius: 5px;\r\n    &:active {\r\n      background: rgba(163, 163, 163, 0.1);\r\n    }\r\n    &:first-child {\r\n      margin-left: 2%;\r\n    }\r\n    .title_pane {\r\n      text-align: center;\r\n    }\r\n    .desc {\r\n      color: #666;\r\n      text-align: center;\r\n      font-size: 12px;\r\n    }\r\n    .slogan {\r\n      text-align: center;\r\n      font-size: 12px;\r\n      color: #999;\r\n      margin-top: 10px;\r\n    }\r\n    span {\r\n      font-size: 12px;\r\n      border: 1px solid #6777fe;\r\n      // display: inline-block;\r\n      padding: 2px;\r\n      transform: scale(0.8);\r\n      white-space: nowrap;\r\n    }\r\n    .title_left {\r\n      color: #fff;\r\n      background: #6777fe;\r\n    }\r\n    .title_right {\r\n      color: #6777fe;\r\n      background: #fff\r\n    }\r\n  }\r\n}\r\n\r\n</style>\r\n"},"lineCount":null}},"error":null,"hash":"f5609171aa652c7cc59913998406b56c","cacheData":{"env":{}}}